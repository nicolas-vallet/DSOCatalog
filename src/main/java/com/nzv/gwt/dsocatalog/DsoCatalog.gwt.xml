<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.2//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to='dso'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.dark.Dark'/>
    <inherits name='com.google.gwt.visualization.Visualization'/>
    <inherits name='com.google.gwt.i18n.I18N'/>
    <entry-point class='com.nzv.gwt.dsocatalog.client.DsoCatalogGWT'/>
    
    <!-- Specify the paths for translatable code                    -->
    <source path='client' />
    <source path='model'  />
    <source path='date'  />
    <source path='visualization'  />
    <source path='projection' />
    <source path='i18n' />
    
    <!-- External resources -->
    <inherits name='com.nzv.astro.meusEngine' />
    <inherits name='com.nzv.astro.ephemeris.planetary.planetaryEphemeris' />
    <inherits name='com.nzv.gwt.ga.GA' />
    
    <extend-property name="locale" values="fr"/>
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="es"/>
    <extend-property name="locale" values="ru"/>
<!--     <extend-property name="locale" values="ar"/> -->
<!--     <extend-property name="locale" values="hi"/> -->
<!--     <extend-property name="locale" values="zh"/> -->
</module>